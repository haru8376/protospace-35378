<main class="main">
  <div class="inner">
    <% if user_signed_in? %>
      <div class="greeting">
        <a>こんにちは、</a><%= link_to current_user.name + "さん", user_path(current_user), class: :greeting__link %>
      </div>
      <div class="card__wrapper">
        <%# 投稿機能実装後、部分テンプレートでプロトタイプ投稿一覧を表示する %>
        <%= render partial: 'prototype', collection: @prototypes %>
      </div>
    <% end %>
  </div>
</main>
